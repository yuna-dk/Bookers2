<%= render 'books/list', books: @books %>

    <div class="col-md-8 offset-md-1">
      <h1>Users</h1>
        <table class="table table-striped">
          <thead>
            <tr>
              <th>image</th>
              <th>name</th>
            </tr>
          </thead>
          <% @users.each do |user| %>
          <tbody>
            <tr>
              <td><%= image_tag @user.get_profile_image(80,80) %></td>
              <td><%= @user.name %></td>
              <td><%= link_to "Show", user_path(@user.id) %></td>
            </tr>
          </tbody>
          <% end %>
          </table>
    </div>
  </div>
</div>